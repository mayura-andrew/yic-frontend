---
// @ts-ignore
import { Icon } from "astro-icon";
import { Image } from 'astro:assets';
import  SIC  from "../Images/sic.png";
import OUSL from "../Images/ousl.png";

---

<div class="mt-20">
  <h2 class="text-center text-gray-800 font-bold text-lg md:text-4xl lg:text-4xl">We are working with</h2>
  <div class="flex gap-8 md:gap-20 items-center justify-center mt-0 flex-wrap">
    <div class="w-30 h-30 md:w-30 md:h-30">
      <Image
        src={SIC}
        alt="SIC Image"
        loading="eager"
        format="png"
      />
    </div>
  </div>
</div>

